{
  "name": "pikachu11",
  "version": "1.0.0",
  "description": "Pikachu11 Discord Bot",
  "main": "pika-bot.js",
  "author": "wenz11",
  "type": "module",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.2.0",
    "dotenv": "^10.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "winston": "^3.3.3"
